{
  "name": "kids",
  "version": "1.0.0",
  "scripts": {
    "build": "run-p build:*",
    "build:tailwindcss": "tailwindcss -i src/_includes/tailwind.scss -o src/_includes/tailwind.css --postcss",
    "build:eleventy": "eleventy --input src --quiet",
    "develop": "run-p develop:*",
    "develop:tailwindcss": "tailwindcss -i src/_includes/tailwind.scss -o src/_includes/tailwind.css --postcss --watch",
    "develop:eleventy": "eleventy --input=src --serve --quiet"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "autoprefixer": "^10.4.17",
    "js-yaml": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1"
  }
}
